<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mirhassanmir.mirhasanapp" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Mir Hasan Mir Offical App</name>
    <description>
       Official App of Mir Hasan Mir
    </description>
    <author email="info@frisson.studio" href="http://frisson.studio/">
      Frisson Studio
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="android-build-tool" value="gradle" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="default" />
    <preference name="SplashScreenDelay" value="4000" />
    <preference name="SplashScreen" value="screen" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>
    <plugin spec="https://github.com/driftyco/ionic-plugins-keyboard.git#011283b" />
    <plugin spec="https://github.com/pushandplay/cordova-plugin-apprate.git#41d40d7" />
    <plugin spec="https://github.com/katzer/cordova-plugin-email-composer.git" />
    <plugin spec="https://github.com/EddyVerbruggen/SocialSharing-PhoneGap-Plugin#01ad072" />
    <plugin spec="https://github.com/apache/cordova-plugin-console#450752b" />
    <plugin spec="https://github.com/apache/cordova-plugin-device#c18039b" />
    <plugin spec="https://github.com/apache/cordova-plugin-inappbrowser#56bb457" />
    <plugin spec="https://github.com/apache/cordova-plugin-splashscreen#14061d1" />
    <plugin name="cordova-plugin-admobpro" spec="2.19.1" />
    <plugin name="cordova-plugin-whitelist" spec="1.2.2" />
    <plugin name="cordova-plugin-x-toast" spec="2.5.2" />
    <plugin name="onesignal-cordova-plugin" spec="1.12.6" />
    <plugin name="cordova-plugin-settings-hook" spec="~0" />
    <icon src="res/icon.png" />
    <splash src="res/screen.png" />
    <icon platform="android" qualifier="ldpi" src="www/res/icon/android/icon-36-ldpi.png" />
    <icon platform="android" qualifier="mdpi" src="www/res/icon/android/icon-48-mdpi.png" />
    <icon platform="android" qualifier="hdpi" src="www/res/icon/android/icon-72-hdpi.png" />
    <icon platform="android" qualifier="xhdpi" src="www/res/icon/android/icon-96-xhdpi.png" />
    <icon platform="blackberry" src="www/res/icon/blackberry/icon-80.png" />
    <icon platform="blackberry" src="www/res/icon/blackberry/icon-80.png" state="hover" />
    <icon height="57" platform="ios" src="www/res/icon/ios/icon-57.png" width="57" />
    <icon height="72" platform="ios" src="www/res/icon/ios/icon-72.png" width="72" />
    <icon height="114" platform="ios" src="www/res/icon/ios/icon-57-2x.png" width="114" />
    <icon height="144" platform="ios" src="www/res/icon/ios/icon-72-2x.png" width="144" />
    <icon height="76" platform="ios" src="www/res/icon/ios/Icon-App-76x76@1x.png" width="76" />
    <icon height="120" platform="ios" src="www/res/icon/ios/Icon-App-120x120@1x.png" width="120" />
    <icon height="152" platform="ios" src="www/res/icon/ios/Icon-App-152x152@1x.png" width="152" />
    <icon platform="webos" src="www/res/icon/webos/icon-64.png" />
    <icon platform="winphone" src="www/res/icon/windows-phone/icon-48.png" />
    <icon platform="winphone" role="background" src="www/res/icon/windows-phone/icon-173-tile.png" />
    <splash platform="android" qualifier="port-ldpi" src="www/res/screen/android/screen-ldpi-portrait.png" />
    <splash platform="android" qualifier="port-mdpi" src="www/res/screen/android/screen-mdpi-portrait.png" />
    <splash platform="android" qualifier="port-hdpi" src="www/res/screen/android/screen-hdpi-portrait.png" />
    <splash platform="android" qualifier="port-xhdpi" src="www/res/screen/android/screen-xhdpi-portrait.png" />
    <splash platform="blackberry" src="www/res/screen/blackberry/screen-225.png" />
    <splash height="480" platform="ios" src="www/res/screen/ios/screen-iphone-portrait.png" width="320" />
    <splash height="960" platform="ios" src="www/res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <splash height="1136" platform="ios" src="www/res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <splash height="1024" platform="ios" src="www/res/screen/ios/screen-ipad-portrait.png" width="768" />
    <splash height="768" platform="ios" src="www/res/screen/ios/screen-ipad-landscape.png" width="1024" />
    <splash platform="winphone" src="www/res/screen/windows-phone/screen-portrait.png" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <config-file parent="NSCalendarsUsageDescription" platform="ios" target="*-Info.plist">
            <string>Advertisement would like to create a calendar event.</string>
        </config-file>
        <config-file parent="NSPhotoLibraryUsageDescription" platform="ios" target="*-Info.plist">
            <string>Advertisement would like to store a photo.</string>
        </config-file>
        <config-file parent="NSBluetoothPeripheralUsageDescription" platform="ios" target="*-Info.plist">
            <string>Advertisement would like to use bluetooth.</string>
        </config-file>
    </platform>
    
    <platform name="winphone">
    </platform>
    <plugin name="cordova-plugin-media-capture" spec="^1.4.3">
        <variable name="CAMERA_USAGE_DESCRIPTION" value="  " />
        <variable name="MICROPHONE_USAGE_DESCRIPTION" value="  " />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="  " />
    </plugin>
    <plugin name="cordova-plugin-ios-ble-permissions" spec="^1.0.1" />
    <engine name="browser" spec="~4.1.0" />
</widget>
